<?xml version="1.0" encoding="utf-8" ?>
<paramlist>
	<!--记录参数的信息 https://www.cnblogs.com/Jacklovely/p/7151423.html -->
	<param PID="1" ParamName="WorkStationCount">
		<DefaultValue>1</DefaultValue>
		<Description>工位数</Description>
		<Remark>工位数</Remark>
	</param>
	<param PID="2" ParamName="TaskCount">
		<DefaultValue>10</DefaultValue>
		<Description>任务数</Description>
		<Remark>任务数</Remark>
	</param>
	<param PID="3" ParamName="TaskIDStartValue">
		<DefaultValue>1</DefaultValue>
		<Description>0或者1</Description>
		<Remark>起始任务号</Remark>
	</param>
	<param PID="4" ParamName="PLC_IP">
		<!--<DefaultValue>10.217.17.5</DefaultValue>-->
    <DefaultValue>192.168.31.254</DefaultValue>
		<Description>PLC网络IP</Description>
		<Remark>PLC网络IP</Remark>;
  </param>
	<param PID="5" ParamName="PLC_Port">
		<DefaultValue>0</DefaultValue>
		<Description>PLC网络端口</Description>
		<Remark>网络端口</Remark>
	</param>
	<param PID="6" ParamName="ScrewMachineCount">
		<DefaultValue>1</DefaultValue>
		<Description>丹尼克尔电批数量</Description>
		<Remark>丹尼克尔电批数量</Remark>
	</param>
	<param PID="7" ParamName="ScrewMachineIP1">
		<DefaultValue>10.217.17.8</DefaultValue>
		<Description>电批网络地址</Description>
		<Remark>电批网络地址</Remark>
	</param>
	<param PID="8" ParamName="ScrewMachinePort1">
		<DefaultValue>5000</DefaultValue>
		<Description>网络端口1</Description>
		<Remark>电批网络端口</Remark>
	</param>
	<param PID="9" ParamName="ScrewMachineIP2">
		<DefaultValue>192.168.100.1</DefaultValue>
		<Description>电批网络地址2</Description>
		<Remark>电批网络地址</Remark>
	</param>
	<param PID="10" ParamName="ScrewMachinePort2">
		<Remark>工位数</Remark>
		<DefaultValue>1030</DefaultValue>
		<Description>电批网络端口2</Description>
		<Remark>电批网络端口</Remark>
	</param>
	<param PID="11" ParamName="ScrewMachineFlag1">
		<DefaultValue>1</DefaultValue>
		<Description>1启用，0禁用</Description>
		<Remark>第一电批状态</Remark>
	</param>
	<param PID="12" ParamName="ScrewMachineFlag2">
		<DefaultValue>0</DefaultValue>
		<Description>1启用，0禁用</Description>
		<Remark>第二电批状态</Remark>
	</param>
	<param PID="13" ParamName="PLC_Rack">
		<DefaultValue>0</DefaultValue>
		<Description>机架</Description>
		<Remark>机架</Remark>
	</param>
	<param PID="14" ParamName="PLC_Slot">
		<DefaultValue>1</DefaultValue>
		<Description>插槽</Description>
		<Remark>插槽</Remark>
	</param>
	<param PID="15" ParamName="PLC_UpLoad1Code">
		<DefaultValue>P</DefaultValue>
		<Description>上传PLC的SN码首字母</Description>
		<Remark>上传PLC的SN码首字母</Remark>
	</param>
	<param PID="16" ParamName="MES_UpLoad1Code">
		<DefaultValue>M</DefaultValue>
		<Description>上传MES的SN码首字母</Description>
		<Remark>上传MES的SN码首字母</Remark>
	</param>
	<param PID="17" ParamName="Checking_PLC_MES">
		<DefaultValue>3</DefaultValue>
		<Description>检查P码和M码，，0不检查，1检查P，2检查M，3都检查</Description>
		<Remark>检查P码和M码</Remark>
	</param>
	<param PID="18" ParamName="MCodeLength">
		<DefaultValue>28</DefaultValue>
		<Description>M码长度，0不检查</Description>
		<Remark>M码长度，0不检查</Remark>
	</param>
</paramlist>